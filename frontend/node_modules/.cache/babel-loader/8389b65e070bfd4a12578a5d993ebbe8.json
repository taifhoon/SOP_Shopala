{"ast":null,"code":"import \"core-js/modules/es.array.includes.js\";\nimport \"core-js/modules/es.string.includes.js\";\nimport \"core-js/modules/es.array.splice.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport axios from '@/plugins/axios';\nexport default {\n  name: \"app\",\n  props: ['user'],\n  data: function data() {\n    return {\n      seat: [],\n      movie: [],\n      seatBook: [],\n      rows: [],\n      cols: [],\n      selectSeats: [],\n      color: null,\n      confirmModal: false,\n      orderId: null,\n      orderType: [],\n      itemId: [],\n      seatId: []\n    };\n  },\n  mounted: function mounted() {\n    window.scrollTo(0, 0);\n    this.getSeat(this.$route.params.id);\n  },\n  computed: {\n    priceOrder: function priceOrder() {\n      var total = 0;\n\n      for (var i = 0; i < this.selectSeats.length; i++) {\n        for (var j = 0; j < this.seat.length; j++) {\n          if (this.seat[j].no_of_seats == this.selectSeats[i]) {\n            total += this.seat[j].type_price;\n          }\n        }\n      }\n\n      return total;\n    }\n  },\n  methods: {\n    getSeat: function getSeat(id) {\n      var _this = this;\n\n      axios.get(\"http://localhost:3000/movie/\".concat(id)).then(function (response) {\n        _this.seat = response.data.seat;\n        _this.movie = response.data.movie[0];\n\n        _this.getSeatBook(response.data.seatBook);\n\n        _this.movie.show_date = new Date(_this.movie.show_date).toDateString();\n\n        if (_this.seat[0].THEATRE_tid == 1 || _this.seat[0].THEATRE_tid == 2) {\n          _this.rows = [\"A\", \"B\", \"C\", \"D\", \"E\"];\n          _this.cols = [\"01\", \"02\", \"03\", \"04\", \"05\", \"06\", \"07\", \"08\", \"09\", \"10\"];\n        } else if (_this.seat[0].THEATRE_tid == 3) {\n          _this.rows = [\"A\", \"B\", \"C\"];\n          _this.cols = [\"01\", \"02\", \"03\", \"04\", \"05\"];\n        }\n      }).catch(function (err) {\n        console.log(err);\n      });\n    },\n    getSeatBook: function getSeatBook(Book) {\n      for (var i = 0; i < Book.length; i++) {\n        this.seatBook.push(Book[i].no_of_seats);\n      }\n    },\n    selectSeat: function selectSeat(r, c) {\n      if (this.selectSeats.includes(r + c)) {\n        this.selectSeats.splice(this.selectSeats.indexOf(r + c), 1);\n      } else {\n        this.selectSeats.push(r + c);\n      }\n    },\n    getType: function getType(r, c) {\n      for (var i = 0; i < this.seat.length; i++) {\n        if (this.seat[i].no_of_seats.includes(r + c)) {\n          var type = i;\n        }\n      }\n\n      return this.seat[type].type_name;\n    },\n    confirmOrder: function confirmOrder(id) {\n      var _this2 = this;\n\n      this.confirmModal = true;\n\n      for (var i = 0; i < this.selectSeats.length; i++) {\n        for (var j = 0; j < this.seat.length; j++) {\n          if (this.seat[j].no_of_seats == this.selectSeats[i]) {\n            this.orderType.push(this.seat[j].type_price);\n          }\n        }\n      }\n\n      this.getSeatId();\n      axios.put(\"http://localhost:3000/movie/confirm/\".concat(id), {\n        selectSeat: this.selectSeats,\n        price: this.priceOrder,\n        type: this.orderType,\n        movie: this.movie,\n        seatId: this.seatId\n      }).then(function (res) {\n        _this2.orderId = res.data.orderId;\n        _this2.itemId = res.data.itemId;\n        console.log(_this2.orderId);\n      }).catch(function (err) {\n        console.log(err);\n      });\n    },\n    closeModal: function closeModal() {\n      this.confirmModal = false;\n      this.orderType = [];\n      axios.delete(\"http://localhost:3000/movie/cancel/\".concat(this.orderId), {\n        itemId: this.itemId\n      }).then(function (res) {\n        res;\n      }).catch(function (err) {\n        console.log(err);\n      });\n    },\n    getSeatId: function getSeatId() {\n      for (var i = 0; i < this.selectSeats.length; i++) {\n        for (var j = 0; j < this.seat.length; j++) {\n          if (this.selectSeats[i] == this.seat[j].no_of_seats) this.seatId.push(this.seat[j].seat_id);\n        }\n      }\n\n      console.log(this.seatId);\n    },\n    image: function image(file_path) {\n      if (file_path) {\n        return \"http://localhost:3000/\" + file_path;\n      } else {\n        return \"https://bulma.io/images/placeholders/640x360.png\";\n      }\n    },\n    isAdmin: function isAdmin() {\n      if (!this.user) return false;\n      return this.user.role == 'admin';\n    },\n    isCusto: function isCusto() {\n      if (!this.user) return false;\n      return this.user.role == 'customer';\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAqKA;AACA;AACAA,aADA;AAEAC,iBAFA;AAGAC,MAHA,kBAGA;AACA;AACAC,cADA;AAEAC,eAFA;AAGAC,kBAHA;AAIAC,cAJA;AAKAC,cALA;AAMAC,qBANA;AAOAC,iBAPA;AAQAC,yBARA;AASAC,mBATA;AAUAC,mBAVA;AAWAC,gBAXA;AAYAC;AAZA;AAcA,GAlBA;AAmBAC,SAnBA,qBAmBA;AACAC;AACA;AACA,GAtBA;AAuBAC;AACAC,cADA,wBACA;AACA;;AAEA;AACA;AACA;AACAC;AACA;AACA;AACA;;AACA;AACA;AAZA,GAvBA;AAqCAC;AACAC,WADA,mBACAC,EADA,EACA;AAAA;;AACAC,YACAC,GADA,uCACAF,EADA,GAEAG,IAFA,CAEA;AACA;AACA;;AACA;;AACA;;AACA;AACA;AACA;AACA,SAHA,MAGA;AACA;AACA;AACA;AACA,OAdA,EAeAC,KAfA,CAeA;AACAC;AACA,OAjBA;AAkBA,KApBA;AAqBAC