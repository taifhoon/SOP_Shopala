{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport axios from '@/plugins/axios';\nexport default {\n  name: \"app\",\n  props: [\"user\"],\n  data: function data() {\n    return {\n      order: [],\n      customer: [],\n      show: [],\n      movies: [],\n      theatre: [],\n      checkPayment: false\n    };\n  },\n  mounted: function mounted() {\n    window.scrollTo(0, 0);\n    this.getOrder(this.$route.params.orderId);\n  },\n  computed: {},\n  methods: {\n    getOrder: function getOrder(id) {\n      var _this = this;\n\n      axios.get(\"http://localhost:3000/booking/\".concat(id)).then(function (response) {\n        console.log(response.data.show);\n        _this.order = response.data.order[0];\n        _this.customer = response.data.customer[0];\n        _this.show = response.data.show[0];\n        _this.movies = response.data.movies[0];\n        _this.theatre = response.data.theatre[0];\n        _this.show.show_date = new Date(_this.show.show_date).toDateString();\n\n        var all_seats = _this.order.order_seats.substring(0, _this.order.order_seats.length - 2);\n\n        _this.order.order_seats = all_seats;\n      }).catch(function (err) {\n        console.log(err);\n      });\n    },\n    payment: function payment(id) {\n      this.checkPayment = true;\n      axios.put(\"http://localhost:3000/booking/\".concat(id)).then(function (response) {\n        console.log(response);\n      }).catch(function (err) {\n        console.log(err);\n      });\n    },\n    image: function image(file_path) {\n      if (file_path) {\n        return \"http://localhost:3000/\" + file_path;\n      } else {\n        return \"https://bulma.io/images/placeholders/640x360.png\";\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgCA;AACA;AACAA,aADA;AAEAC,iBAFA;AAGAC,MAHA,kBAGA;AACA;AACAC,eADA;AAEAC,kBAFA;AAGAC,cAHA;AAIAC,gBAJA;AAKAC,iBALA;AAMAC;A